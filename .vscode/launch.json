{
  "version": "0.2.0",
  "configurations": [
    {
			"name": "Chrome Tasks",
			"type": "extensionHost",
			"request": "launch",
      "preLaunchTask": "vscode:watch",
			"runtimeExecutable": "${execPath}",
			"args": [
        "--disable-extensions",
        "--extensionDevelopmentPath=${workspaceFolder}/out/vscode",
        "${workspaceFolder}/examples/all.code-workspace",
      ],
			"sourceMaps": true,
			"outFiles": [
        "${workspaceFolder}/out/vscode/**/*.js",
      ],
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**",
      ],
		},
  ],
}
